{
  "name": "ansible_forms",
  "version": "5.0.5",
  "repository": {
    "type": "git",
    "url": "git://github.com/ansibleguy76/ansibleforms.git"
  },
  "description": "A forms frontend for ansible and awx",
  "scripts": {
    "start": "npm run prod",
    "build": "npm-run-all clean transpile",
    "copy": "cp .env.development ./dist/.env.development",
    "server": "node ./dist",
    "dev": "NODE_ENV=development npm-run-all build copy server",
    "prod": "NODE_ENV=production npm-run-all build server",
    "transpile": "babel ./ --ignore node_modules --out-dir dist --copy-files",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@outlinewiki/passport-azure-ad-oauth2": "~0.1.0",
    "ajv": "~6.12.6",
    "ajv-error-parser": "~1.0.7",
    "axios": "~1.7.7",
    "bcrypt": "~5.1.0",
    "bluebird": "~3.7.2",
    "cert-info": "~1.5.1",
    "cheerio": "~1.0.0",
    "connect-history-api-fallback": "~2.0.0",
    "core-js": "~3.38.1",
    "cors": "~2.8.5",
    "cron-parser": "~4.9.0",
    "dayjs": "~1.11.13",
    "express": "~4.21.0",
    "cookie-session": "~2.1.0",
    "fs-extra": "~11.2.0",
    "ip": "2.0.1",
    "json-bigint": "~1.0.0",
    "ldap-authentication": "~3.2.4",
    "ldapjs": "~3.0.7",
    "lodash": "~4.17.21",
    "modern-passport-http": "~0.3.0",
    "moment": "~2.30.1",
    "mongodb": "~6.9.0",
    "oracledb": "~6.6.0",
    "mssql": "~10.0.2",
    "multer": "~1.4.5-lts.1",
    "mysql2": "~3.11.3",
    "node-cache": "~5.1.2",
    "node-jq": "~4.4.0",
    "nodemailer": "~6.9.15",
    "openid-client": "^5.7.0",
    "passport": "~0.7.0",
    "passport-jwt": "~4.0.1",
    "pg": "~8.13.0",
    "read-last-lines": "~1.8.0",
    "swagger-ui-express": "~5.0.1",
    "thenby": "~1.3.4",
    "winston": "~3.14.2",
    "winston-daily-rotate-file": "~5.0.0",
    "winston-syslog": "~2.7.1",
    "yaml": "~2.5.0"
  },
  "devDependencies": {
    "@babel/cli": "~7.24.7",
    "@babel/core": "7.25.2",
    "@babel/eslint-parser": "7.25.1",
    "@babel/node": "~7.25.0",
    "dotenv": "~16.4.1",
    "eslint": "~8.56.0",
    "nodemon": "~3.1.5",
    "npm-run-all": "*",
    "rifraf": "~2.0.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-unused-vars": "off"
    }
  },
  "author": "ansibleguy",
  "license": "ISC"
}
