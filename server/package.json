{
  "name": "ansible_forms",
  "version": "5.0.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/ansibleguy76/ansibleforms.git"
  },
  "description": "A forms frontend for ansible and awx",
  "scripts": {
    "start": "npm run prod",
    "build": "npm-run-all clean transpile",
    "copy": "cp .env.development ./dist/.env.development",
    "server": "node ./dist",
    "dev": "NODE_ENV=development npm-run-all build copy server",
    "prod": "NODE_ENV=production npm-run-all build server",
    "transpile": "babel ./ --ignore node_modules --out-dir dist --copy-files",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "core-js": "~3.33.2",
    "axios": "~1.6.0",
    "cors": "~2.8.5",
    "bcrypt": "~5.1.0",
    "cheerio": "~1.0.0-rc.12",
    "connect-history-api-fallback": "~2.0.0",
    "express": "~4.18.2",
    "multer": "~1.4.5-lts.1",
    "express-session": "~1.17.3",
    "winston": "~3.11.0",
    "winston-syslog": "~2.7.0",
    "winston-daily-rotate-file": "~4.7.1",
    "passport": "~0.6.0",
    "modern-passport-http": "~0.3.0",
    "passport-jwt": "~4.0.1",
    "@outlinewiki/passport-azure-ad-oauth2": "~0.1.0",
    "mysql2": "~3.6.2",
    "ajv": "~6.12.6",
    "ajv-error-parser": "~1.0.7",
    "lodash": "~4.17.21",
    "bluebird": "~3.7.2",
    "node-jq": "~4.2.2",
    "moment": "~2.29.4",
    "yaml": "~2.3.4",
    "cert-info":"~1.5.1",
    "thenby":"~1.3.4",
    "mssql":"~10.0.1",
    "mongodb":"~6.2.0",
    "pg":"~8.11.3",
    "ip":"1.1.8",
    "dayjs": "1.11.10",
    "fs-extra":"~11.1.1",
    "node-cache":"~5.1.2",
    "json-bigint":"~1.0.0",
    "ldap-authentication": "~3.0.3",
    "ldapjs": "~3.0.5",
    "read-last-lines": "~1.8.0",
    "swagger-ui-express": "~5.0.0",
    "nodemailer": "~6.9.7",
    "nanoid": "~3.3.7"
  },
  "devDependencies": {
    "@babel/core": "7.23.2",
    "@babel/cli": "~7.23.0",
    "@babel/node": "~7.22.19",
    "@babel/eslint-parser": "7.22.15",
    "nodemon": "~3.0.1",
    "rifraf": "~2.0.3",
    "npm-run-all": "*",
    "dotenv": "~16.3.1",
    "eslint": "~8.53.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-unused-vars": "off"
    }
  },
  "author": "ansibleguy",
  "license": "ISC"
}
