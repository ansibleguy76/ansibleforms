{
  "name": "ansible-forms",
  "version": "6.1.1",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git://github.com/ansibleguy76/ansibleforms.git"
  },
  "description": "The nodejs backend for the ansible forms web applications",
  "scripts": {
    "start": "npm run prod",
    "build": "npm-run-all clean transpile",
    "copy": "cp .env.development ./dist/.env.development",
    "server": "NODE_ENV=development node ./",
    "dev": "NODE_ENV=development nodemon --watch src --watch schema --watch config ./src/index.js",
    "prod": "NODE_ENV=production npm-run-all build server",
    "copyviews": "mkdir -p ./dist/views && cp -R ../client/dist/* ./dist/views/",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@outlinewiki/passport-azure-ad-oauth2": "~0.1.0",
    "ajv": "~8.17.1",
    "axios": "~1.13.2",
    "bcrypt": "~6.0.0",
    "cert-info": "~1.5.1",
    "connect-history-api-fallback": "~2.0.0",
    "cookie-session": "~2.1.1",
    "core-js": "~3.48.0",
    "cors": "~2.8.6",
    "cron-parser": "~5.5.0",
    "dayjs": "~1.11.19",
    "dotenv": "~17.2.1",
    "express": "~5.2.1",
    "fs-extra": "~11.3.3",
    "json-bigint": "~1.0.0",
    "ldap-authentication": "~3.3.6",
    "ldapts": "~8.1.3",
    "luxon": "3.7.2",
    "modern-passport-http": "~0.3.0",
    "moment": "~2.30.1",
    "lodash": "4.17.23",
    "moment-timezone": "0.6.0",
    "mongodb": "~7.0.0",
    "mssql": "~12.2.0",
    "multer": "~2.0.2",
    "mysql2": "~3.16.1",
    "node-cache": "~5.1.2",
    "node-jq": "~6.3.1",
    "nodemailer": "~7.0.12",
    "openid-client": "^6.8.1",
    "oracledb": "~6.10.0",
    "passport": "~0.7.0",
    "passport-jwt": "~4.0.1",
    "pg": "~8.17.2",
    "read-last-lines": "~1.8.0",
    "shell-quote": "1.8.3",
    "swagger-ui-express": "~5.0.1",
    "thenby": "~1.3.4",
    "winston": "~3.19.0",
    "winston-daily-rotate-file": "~5.0.0",
    "winston-syslog": "~2.7.1",
    "yaml": "~2.8.2"
  },
  "devDependencies": {
    "eslint": "~9.39.2",
    "nodemon": "~3.1.11",
    "npm-run-all": "*",
    "rifraf": "~2.0.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "es2022": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "rules": {
      "no-unused-vars": "off"
    }
  },
  "author": "ansibleguy",
  "license": "GPL"
}
