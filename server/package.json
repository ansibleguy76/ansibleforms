{
  "name": "ansible_forms",
  "version": "3.0.9",
  "repository": {
    "type": "git",
    "url": "git://github.com/ansibleguy76/ansibleforms.git"
  },
  "description": "A forms frontend for ansible and awx",
  "scripts": {
    "start": "npm run prod",
    "build": "npm-run-all clean transpile",
    "copy": "cp .env.development ./dist/.env.development",
    "server": "node ./dist",
    "dev": "NODE_ENV=development npm-run-all build copy server",
    "prod": "NODE_ENV=production npm-run-all build server",
    "transpile": "babel ./ --ignore node_modules --out-dir dist --copy-files",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "core-js": "~3.25.3",
    "axios": "~0.27.2",
    "cors": "~2.8.5",
    "bcrypt": "~5.0.1",
    "cheerio": "~1.0.0-rc.12",
    "connect-history-api-fallback": "~2.0.0",
    "express": "~4.18.1",
    "winston": "~3.8.2",
    "winston-syslog": "~2.6.0",
    "passport": "~0.6.0",
    "passport-http": "~0.3.0",
    "passport-jwt": "~4.0.0",
    "mysql": "~2.18.1",
    "ajv": "~6.12.6",
    "ajv-error-parser": "~1.0.7",
    "bluebird": "~3.7.2",
    "node-jq": "~2.3.3",
    "moment": "~2.29.4",
    "yaml": "~2.1.1",
    "cert-info":"~1.5.1",
    "thenby":"~1.3.4",
    "mssql":"~9.0.1",
    "mongodb":"~4.10.0",
    "pg":"~8.8.0",
    "fs-extra":"~10.1.0",
    "node-cache":"~5.1.2",
    "ldap-authentication": "~2.3.1",
    "ldapjs": "~2.3.3",
    "read-last-lines": "~1.8.0",
    "swagger-ui-express": "~4.5.0",
    "nodemailer": "~6.7.8"
  },
  "devDependencies": {
    "@babel/core": "7.19.1",
    "@babel/cli": "~7.18.10",
    "@babel/node": "~7.19.1",
    "@babel/eslint-parser": "7.19.1",
    "nodemon": "~2.0.20",
    "rifraf": "~2.0.3",
    "npm-run-all": "*",
    "dotenv": "~16.0.2",
    "eslint": "~8.24.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-unused-vars": "off"
    }
  },
  "author": "ansibleguy",
  "license": "ISC"
}
