{
  "name": "ansible_forms",
  "version": "6.0.0",
  
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git://github.com/ansibleguy76/ansibleforms.git"
  },
  "description": "A forms frontend for ansible and awx",
  "scripts": {
    "start": "npm run prod",
    "build": "npm-run-all clean transpile",
    "copy": "cp .env.development ./dist/.env.development",
    "server": "NODE_ENV=development node ./",
    "dev": "NODE_ENV=development nodemon --watch src --watch schema --watch config ./src/index.js",
    "prod": "NODE_ENV=production npm-run-all build server",
    "copyviews": "mkdir -p ./dist/views && cp -R ../client/dist/* ./dist/views/",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@outlinewiki/passport-azure-ad-oauth2": "~0.1.0",
    "ajv": "~8.17.1",
    "axios": "~1.11.0",
    "bcrypt": "~6.0.0",
    "cert-info": "~1.5.1",
    "connect-history-api-fallback": "~2.0.0",
    "core-js": "~3.45.0",
    "cors": "~2.8.5",
    "cron-parser": "~5.3.0",
    "dayjs": "~1.11.13",
    "express": "~5.1.0",
    "cookie-session": "~2.1.1",
    "fs-extra": "~11.3.1",
    "json-bigint": "~1.0.0",
    "ldap-authentication": "~3.3.4",
    "ldapts": "~8.0.9",
    "modern-passport-http": "~0.3.0",
    "moment": "~2.30.1",
    "moment-timezone": "0.6.0",
    "mongodb": "~6.18.0",
    "oracledb": "~6.9.0",
    "mssql": "~11.0.1",
    "multer": "~2.0.2",
    "mysql2": "~3.14.3",
    "node-cache": "~5.1.2",
    "node-jq": "~6.0.1",
    "nodemailer": "~7.0.5",
    "openid-client": "^6.6.3",
    "passport": "~0.7.0",
    "passport-jwt": "~4.0.1",
    "pg": "~8.16.3",
    "read-last-lines": "~1.8.0",
    "swagger-ui-express": "~5.0.1",
    "thenby": "~1.3.4",
    "shell-quote": "1.8.3",
    "winston": "~3.17.0",
    "winston-daily-rotate-file": "~5.0.0",
    "winston-syslog": "~2.7.1",
    "yaml": "~2.8.1",
    "dotenv": "~17.2.1"    
  },
  "devDependencies": {
    "eslint": "~9.33.0",
    "nodemon": "~3.1.10",
    "npm-run-all": "*",
    "rifraf": "~2.0.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "es2022": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "rules": {
      "no-unused-vars": "off"
    }
  },
  "author": "ansibleguy",
  "license": "ISC"
}
