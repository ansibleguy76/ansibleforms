{
  "name": "ansible_forms",
  "version": "1.1.3",
  "description": "A forms frontend for ansible and awx",
  "scripts": {
    "start": "npm run prod",
    "build": "npm-run-all clean transpile",
    "copy": "cp .env.development ./dist/.env.development",
    "server": "node ./dist",
    "dev": "NODE_ENV=development npm-run-all build copy server",
    "prod": "NODE_ENV=production npm-run-all build server",
    "transpile": "babel ./ --ignore node_modules --out-dir dist --copy-files",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "bcrypt": "^5.0.1",
    "cheerio": "^1.0.0-rc.10",
    "connect-history-api-fallback": "^1.6.0",
    "express": "^4.17.1",
    "winston": "^3.3.3",
    "passport": "^0.4.1",
    "passport-http": "^0.3.0",
    "passport-jwt": "^4.0.0",
    "mysql": "^2.18.1",
    "ajv": "^6.12.6",
    "ajv-error-parser": "^1.0.7",
    "bluebird": "^3.7.2",
    "node-jq": "^2.0.0",
    "moment": "^2.29.1",
    "yaml": "^1.10.2",
    "cert-info":"^1.5.1",
    "mssql":"^7.3.0",
    "mongodb":"^4.2.1",
    "crypto":"^1.0.1",
    "pg":"^8.7.1",
    "node-cache":"^5.1.2",
    "ldap-authentication": "^2.2.7",
    "ldapjs": "^2.3.0",
    "swagger-ui-express": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "*",
    "@babel/cli": "*",
    "@babel/node": "*",
    "nodemon": "~2.0.12",
    "rifraf": "~2.0.3",
    "npm-run-all": "*",
    "@babel/eslint-parser": "*",
    "dotenv": "^10.0.0",
    "eslint": "*"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-unused-vars": "off"
    }
  },
  "author": "ansibleguy",
  "license": "ISC"
}
