    "vuelidate": "~0.7.7",
    "vue-router": "~3.5.4",
    "vue-toastification" : "1.7.14",
    "vue-json-pretty" : "1.8.3",
    "copy-to-clipboard" : "~3.3.2",
    "bulma" : "0.9.4",
    "bulma-quickview" : "*",
    "bulma-checkradio": "~2.1.3",
    "bulma-pageloader": "~0.3.0",
    "brace": "~0.11.1",
    "highlight.js": "9.11.0",
    "vue-highlight.js": "3.1.0",
    "vue-bulma-paginate" : "*",
    "vue-moment" : "*",
    "yaml": "1.10.2",
    "thenby": "*",
    "vue-showdown": "2.4.1",
    "@fortawesome/fontawesome-svg-core":"~6.2.0",
