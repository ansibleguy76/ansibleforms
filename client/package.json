{
  "name": "ansible_forms_vue",
  "version": "5.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "remove": "rm -rdf ./../server/views || exit 0",
    "copy": "cp -R dist ../server/views && cp ./../docs/_data/help.yaml ../server",
    "bundle": "npm run build && npm run remove && npm run copy"
  },
  "dependencies": {
    "@creativebulma/bulma-tooltip": "*",
    "@femessage/log-viewer": "*",
    "@fortawesome/fontawesome-svg-core": "~6.7.2",
    "@fortawesome/free-brands-svg-icons": "~6.7.2",
    "@fortawesome/free-regular-svg-icons": "~6.7.2",
    "@fortawesome/free-solid-svg-icons": "~6.7.2",
    "@fortawesome/vue-fontawesome": "2.0.10",
    "axios": "~1.9.0",
    "brace": "~0.11.1",
    "bulma": "1.0.4",
    "copy-to-clipboard": "~3.3.3",
    "core-js": "~3.43.0",
    "es6-promise": "~4.2.8",
    "highlight.js": "^10.7.2",
    "jwt-decode": "4.0.0",
    "lodash": "~4.17.21",
    "thenby": "*",
    "vue": "~2.7.15",
    "@highlightjs/vue-plugin": "1.0.2",
    "vue-json-pretty": "1.8.3",
    "vue-moment": "*",
    "vue-router": "~3.5.4",
    "vue-showdown": "2.4.1",
    "vue-toastification": "1.7.14",
    "vue2-datepicker": "~3.11.1",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "yaml": "2.8.0"
  },
  "devDependencies": {
    "eslint": "~6.8.0",
    "eslint-plugin-vue": "~6.2.2",
    "sass": "~1.89.2",
    "@vitejs/plugin-vue2": "*",
    "vite-plugin-node-polyfills": "0.23.0",
    "@babel/runtime": "7.27.6"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-unused-vars": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
