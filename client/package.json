    "babel-eslint": "~10.1.0",
    "eslint": "~6.8.0",
    "eslint-plugin-vue": "~6.2.2",
    "vue-template-compiler": "~2.6.11",
    "nodemon": "~2.0.16",
    "sass" :"~1.49.11",
    "sass-loader":"10.1.1"
